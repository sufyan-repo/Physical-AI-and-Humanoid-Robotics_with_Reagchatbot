{
  "functions": {
    "api/api.py": {
      "runtime": "python3.12"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/api.py"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "builds": [
    {
      "src": "api/api.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.12"
      }
    }
  ]
}